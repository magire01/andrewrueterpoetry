 default Films;","/Users/markgire/Desktop/andrewrueterpoetry/src/components/SliderComp/SliderItems/Icy.js",["77","78","79","80"],"import React from \"react\";\nimport icyimg from \"../../../assets/icyrueter.png\"\nimport amazonimg from \"../../../assets/amazonlogo.png\"\nimport { Grid, Button, Card, CardActionArea, CardMedia, Typography } from \"@material-ui/core\";\n\nconst Icy = () => {\n\n    const style = {\n        background: {\n            backgroundColor: \"black\",\n            color: \"white\",\n            height: \"100%\",\n            width: \"auto\"\n        },\n        img: {\n            height: 200,\n            width: \"auto\",\n            paddingTop: \"2%\"\n        },\n        button: {\n            height: 80,\n            width: \"auto\",\n            paddingTop: 5\n\n        }\n    }\n    return (\n        <a href=\"https://www.amazon.com/Icy-Andrew-Rueter-ebook/dp/B07VDLZT9Y\" target=\"_blank\">\n            <Card style={style.background}>\n                <CardActionArea>\n                    <CardMedia>\n                        <Grid container direction=\"row\" alignItems=\"center\" justifyContent=\"center\">\n                            <Grid item md={12} xs={12}>\n                                <Typography variant=\"h6\" align=\"center\">Download 'Icy' on Amazon</Typography>\n                            </Grid>\n                            <Grid item md={12} xs={12}>\n                                <img style={style.img} src={icyimg} />\n                            </Grid>\n                            {/* <Grid item md={12} xs={12}>\n                                <img style={style.button} src={amazonimg} />\n                            </Grid> */}\n                        </Grid>\n                    </CardMedia>\n                </CardActionArea>\n            </Card>\n        </a>\n                \n    )\n}\n\nexport default Icy;","/Users/markgire/Desktop/andrewrueterpoetry/src/components/SliderComp/SliderItems/FeaturedPoems.js",["